{
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4,
    "useTabs": true,
    "htmlWhitespaceSensitivity": "ignore",
    "jsxBracketSameLine": true,
    "printWidth": 128,
    "overrides": [
        {
            "files": "*.html",
            "options": {
                "parser":"html"
            }
        },
        {
            "files": "*.vue",
            "options": {
                "parser":"vue"
            }
        }
    ]
}